{
    "name": "qubiqx/qcommerce-ecommerce-mollie",
    "description": "This is my package qcommerce-ecommerce-mollie",
    "keywords": [
        "Qubiqx",
        "laravel",
        "qcommerce-ecommerce-mollie"
    ],
    "homepage": "https://github.com/qubiqx/qcommerce-ecommerce-mollie",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@qubiqx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15",
        "mollie/laravel-mollie": "^2.23",
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Qubiqx\\QcommerceEcommerceMollie\\": "src",
            "Qubiqx\\QcommerceEcommerceMollie\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubiqx\\QcommerceEcommerceMollie\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubiqx\\QcommerceEcommerceMollie\\QcommerceEcommerceMollieServiceProvider"
            ],
            "aliases": {
                "QcommerceEcommerceMollie": "Qubiqx\\QcommerceEcommerceMollie\\Facades\\QcommerceEcommerceMollie"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
